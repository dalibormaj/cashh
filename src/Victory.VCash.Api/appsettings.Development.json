{
  "ApplicationName": "Victory.VCash.Api",
  "DatabaseSettings": {
    "ConnectionStrings": {
      "Postgres": {
        "VCash": "Server=localhost;Port=5432;Database=victory_vcash;User Id=postgres;Password=admin;CommandTimeout=30;"
      }
    },
    "Redis": {
      "Host": "10.10.14.102",
      "Port": 6379,
      "ConnectTimeout": 5000,
      "SyncTimeout": 1000,
      "KeepAlive": 30,
      "AllowAdmin": true,
      "AbortOnConnectFail": false
    }
  },
  "AuthSettings": {
    "Guardian": {
      "Url": "http://auth-api-betagy.betagy.local"
    },
    "AzureAd": {
      "OpenIdConfigUrl": "https://login.microsoftonline.com/victory.rs/v2.0/.well-known/openid-configuration"
    }
  },
  "InternalApiSettings": {
    "Url": "http://intsvc-vic247srb.betagy.local",
    "SaltKey": "2tPQff16A0"
  },
  "PlatformWebSiteApiSettings": {
    "Url": "http://website-api-vic247srb.betagy.local"
  },
  "ConsumerSettings": {
    "Enable": false,
    "RabbitMq": {
      "Host": "172.172.33.141",
      "Port": "5672",
      "VirtualHost": "platform",
      "UserName": "platform",
      "Password": "vagrant",
      "DelayNackSeconds": 10,
      "MaximumThreads": 20,
      "ClientProvidedName": "app:VCashApi component:event-consumer",
      "Queues": {
        "BetDetails": "platform.production.default.betdetails.vnet.queue",
        "UserDetails": "users.updatenotifications.queue"
      }
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.RabbitMQ" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "RabbitMQ",
        "Args": {
          "SinkConfiguration": {
            "BatchPostingLimit": 100,
            "TextFormatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
          },
          "ClientConfiguration": {
            "Hostnames": [
              "172.172.33.141"
            ],
            "Exchange": "logging.exchange.platform.staging.operation",
            "ExchangeType": "fanout",
            "DeliveryMode": "Durable",
            "Port": 5672,
            "VHost": "platform",
            "Username": "platform",
            "Password": "vagrant",
            "UseBackgroundThreadsForIO": true
          }
        }
      },
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId", "WithExceptionDetails" ],
    "Properties": {
      "Application": "Victory.VCash.Api",
      "ReleaseNumber": "Local"
    }
  },
  "AllowedHosts": "*"
}
